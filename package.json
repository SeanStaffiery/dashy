{
  "name": "dashy",
  "version": "3.1.1",
  "license": "MIT",
  "main": "server",
  "author": "Alicia Sykes <alicia@omg.lol> (https://aliciasykes.com)",
  "scripts": {
    "start": "node server",
    "dev": "NODE_OPTIONS=--openssl-legacy-provider vue-cli-service serve",
    "build": "NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build",
    "lint": "vue-cli-service lint",
    "pm2-start": "npx pm2 start server.js",
    "build-watch": "vue-cli-service build --watch --mode production",
    "build-and-start": "NODE_OPTIONS=--openssl-legacy-provider npm-run-all --parallel build start",
    "validate-config": "node services/config-validator",
    "health-check": "node services/healthcheck",
    "dependency-audit": "npx improved-yarn-audit --ignore-dev-deps"
  },
  "dependencies": {
    "@babel/core": "^7.22.0",
    "@formschema/native": "^2.0.0-beta.6",
    "@sentry/tracing": "^8.0.0",
    "@sentry/vue": "^8.0.0",
    "ajv": "^8.12.0",
    "axios": "^1.4.0",
    "connect-history-api-fallback": "^2.0.0",
    "crypto-js": "^4.2.0",
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.1",
    "frappe-charts": "^2.0.0",
    "js-yaml": "^4.1.0",
    "keycloak-js": "^21.0.0",
    "oidc-client-ts": "^4.0.0",
    "remedial": "^1.0.8",
    "rss-parser": "^3.14.0",
    "rsup-progress": "^4.0.0",
    "simple-icons": "^13.0.0",
    "v-jsoneditor": "^2.0.0",
    "v-tooltip": "^4.0.0",
    "v-modal": "^2.0.0",
    "v-tabs": "^1.0.0",
    "v-select": "^4.1.0",
    "v-swatches": "^3.0.0",
    "v-toasted": "^2.0.0",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-js-modal": "^2.0.1",
    "vue-json-tree-view": "^3.0.0",
    "vue-material-tabs": "0.1.5",
    "vue-router": "^4.2.0",
    "vue-select": "^4.1.0",
    "vue-swatches": "^3.0.0",
    "vue-toasted": "^2.0.0",
    "vuex": "^4.1.0",
    "register-service-worker": "^1.8.0",
    "bcrypt": "^6.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.0",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-pwa": "^5.0.8",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-standard": "^6.0.0",
    "@babel/eslint-parser": "^7.22.5",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.46.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-plugin-vue": "^9.18.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.66.0",
    "sass-loader": "^13.3.0",
    "typescript": "^5.5.2",
    "vue-cli-plugin-yaml": "^1.0.2",
    "vue-svg-loader": "0.17.0",
    "@vue/compiler-sfc": "^3.3.4"
  },
  "engines": {
    "node": ">=16.0.0 <21.6.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard",
      "airbnb-base"
    ],
    "rules": {
      "import/no-unresolved": "off",
      "import/extensions": "off",
      "arrow-parens": 0,
      "no-else-return": 0
    },
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    }
  },
  "babel": {
    "presets": [
      "@vue/app",
      "@babel/preset-env"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
